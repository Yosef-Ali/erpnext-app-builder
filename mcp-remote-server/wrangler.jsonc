{
  "name": "erpnext-app-builder-mcp",
  "main": "src/index.ts",
  "compatibility_date": "2024-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "ENVIRONMENT": "production"
  },
  "kv_namespaces": [
    {
      "binding": "OAUTH_KV",
      "id": "<Add-KV-ID>",
      "preview_id": "<Add-Preview-KV-ID>"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "FRAPPE_MCP_SERVER",
        "class_name": "FrappeMCPServer"
      }
    ]
  }
}
